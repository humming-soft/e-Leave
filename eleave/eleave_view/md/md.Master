                        $('#info').text(data.d);
                    }
                    else {
                        $('#info').text("");
                    }
                },
                error: function (XMLHttpRequest, textStatus, errorThrown) {
                    //alert("error notification badge");
                }
            });
        }
        setInterval(updateAlerts, 10000); // 10 seconds
    </script>

    <script type="text/javascript">
        function sessiontimeout() {
            $.sessionTimeout({
                heading: 'h5',
                title: 'Session Timeout',
